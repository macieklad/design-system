{
  "name": "@airhelp/react-styles",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tailwindcss -i ./src/base.css -o ./dist/base.css --minify",
    "dev": "concurrently \"tailwindcss -i ./src/base.css -o ./dist/base.css --watch\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "postcss": "^8.4.18",
    "postcss-import": "^15.0.0",
    "tailwindcss": "^3.2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
